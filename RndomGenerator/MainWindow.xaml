<Window x:Class="RndomGenerator.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:RndomGenerator"
		mc:Ignorable="d"
		Title="iCIO2017" Height="800" Width="1200" KeyDown="OnKeyDown" WindowState="Maximized" WindowStartupLocation="CenterScreen" WindowStyle="None">
	<Window.Background>
		<ImageBrush ImageSource="Resource/BACKGROUND2.png" />
	</Window.Background>
	<DockPanel TextBlock.FontSize="400">
		<StackPanel
			Orientation="Horizontal"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			>
			<Label
				Margin="0, 0, 0, 0"
				Content="0"
				x:Name="lblFirstNumber"
				/>
			<Label
				Margin="20, 0, 0, 0"
				Content="0"
				x:Name="lblSecondNumber"
				/>
			<Label
				Margin="20, 0, 0, 0"
				Content="0"
				x:Name="lblThirdNumber"
				/>
			<Label
				Margin="20, 0, 0, 0"
				Content="0"
				x:Name="lblFourthNumber"
				/>
		</StackPanel>
	</DockPanel>
</Window>
